    "\n",
    "# SQL query se data fetch karein\n",
    "df = pd.read_sql_query(\"SELECT * FROM bookings\", conn)\n",
    "\n",
    "# Connection close karein\n",
    "conn.close()\n",
    "\n",
    "# DataFrame print karein\n",
    "print(df)\n"
